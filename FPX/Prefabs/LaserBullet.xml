    <Scale XYZ="0.1"/>
  </Transform>
  <MeshRenderer>
    <Model Name="Models//Sphere"/>
  </MeshRenderer>
  <Material>
    <DiffuseColor Name="Blue"/>
    <SpecularColor Name="White"/>
  </Material>
  <Rigidbody/>
